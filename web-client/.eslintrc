{
  "extends": [
    "next",
    "next/core-web-vitals",
    "prettier"
  ],
  "rules": {
    "semi": ["error", "never"],
    "quotes": ["error", "single"],
    "eqeqeq": ["error", "smart"],
    "no-useless-return": "warn",
    "no-trailing-spaces": "error",
    "sort-imports": ["error"],
    "no-duplicate-imports": "error",
    "block-spacing": "error",
    "semi-spacing": "error"
  }
}
